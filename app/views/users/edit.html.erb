<% @page_title = "Edit User" %>

<%= link_to '<< Back to List', {:action => 'index'} %>

<div class="users new">
  <h2>Edit User</h2>

  <%= form_for(:user, :url => {:action => 'update', :id => @user.id}) do |f| %>
  
    <%= render(:partial => "form", :locals => {:f => f}) %>
    
    <div class="form-buttons">
      <%= submit_tag("Edit User") %>
    </div>
    
  <% end %>
</div>
